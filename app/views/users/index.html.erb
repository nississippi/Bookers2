<h1>Users info</h1>
<div>
  <%= image_tag current_user.get_profile_image %>
  <%= current_user.name %>
  <%= current_user.introduction %>
  <%= link_to "UserEdit", edit_user_path(current_user.id) %>
</div>

<h1>New book</h1>
  <%= form_with model: @book do |f| %>
    <%= f.label :title %><br />
    <%= f.text_field :title %><br />
    <%= f.label :body, "Opinion" %><br />
    <%= f.text_area :body %><br />
    <%= f.submit'Create Book' %>
  <% end %>

<% @users.each do |user| %>
    <div>
      <%= image_tag user.get_profile_image %>
      <%= user.name %>
      <%= link_to "Show", user_path(user.id) %>
    </div>
  <% end %>